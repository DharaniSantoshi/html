<script type="text/javascript">
google.charts.load('current',{packages':['corechart']}0;
google.charts.setOnLoadcallback(drawfunction);
function drawfunction(){
var data=google.visualization.arrayToDataTable([
["tasks","time"],
["break",""35"],
["work","65"],
]);
var options={title:'My Chart"};
var char=new google.visualization.piechart(dopcument.getElementById('box'));
char.draw(data,options);
}
</script>
</head>
